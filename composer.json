{
    "name": "evervault/evervault-php",
    "description": "Evervault Bindings for PHP",
    "type": "library",
    "require": {
        "php": "^7.1|^8.0",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "shanecurran/phpecc": "^0.0.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Evervault",
            "email": "hey@evervault.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Evervault\\": "src/Evervault"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Evervault\\Tests\\": "tests/Evervault"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0||^8.0||^9.0"
    }
}
